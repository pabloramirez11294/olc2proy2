/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "node"
    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "src/app/Entornos/*.ts",
    "src/app/Expresiones/*.ts",
    "src/app/Instruccion/*.ts",
    "src/app/Modelos/*.ts",
    "src/app/Reportes/*.ts",
    "src/app/Estructuras/*.ts"
  ]
}
